<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="NetDisk" />
    <style name="NetDisk.TextAppearance" parent="@style/NetDisk" />
    
   <style name="NetDisk.TextAppearance.Large" parent="@style/NetDisk.TextAppearance">
        <item name="android:textSize">@dimen/text_size_large</item>
    </style>
     <style name="NetDisk.TextAppearance.Large.Black" parent="@style/NetDisk.TextAppearance.Large">
        <item name="android:textColor">@color/blue_black</item>
    </style>
    
    
            <style name="dialog_title_bar">
        <item name="android:background">#ff28b1e5</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">52dp</item>
        
    </style>
    

        <style name="Dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    
        <style name="CusterDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:layout_width">100dip</item>
        <item name="android:layout_height">100dip</item>
    </style>

        
      <style name="dialog_button_generic">
        
        <item name="android:background" >@drawable/homepage_settings_bg_selector</item >
        <item name="android:gravity" >center</item >
        <item name="android:textSize" >18dp</item >
        <item name="android:textColor" >@color/selector_color_text_default_first</item >
        <item name="android:singleLine" >true</item >
        
    </style>
    
           <style name="DialogWallpaper" parent="android:style/Theme.Wallpaper">
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
 	</style>

      
</resources>
