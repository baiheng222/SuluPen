<keyboard
  skb_template="@xml/skb_template1"
  skb_cache_flag="true"
  width="10%p"
  height="25%p"
  key_type="0"
  repeat="false"
  balloon="true">

  <row>
    <key label="【">
      <toggle_state state_id="@string/toggle_en_sym2" label="["/>
    </key>
    <key label="】">
      <toggle_state state_id="@string/toggle_en_sym2" label="]"/>
    </key>
    <keys splitter="|" labels="{|}|#|%|^|*|+|="></keys>
   </row>
    
  <row>
      <key label="-"/>
    <key label="—">
      <toggle_state state_id="@string/toggle_en_sym2" label="\\"/>
    </key>
    <key label="\\ ">
      <toggle_state state_id="@string/toggle_en_sym2" label="|"/>
    </key>
    <key label="|">
      <toggle_state state_id="@string/toggle_en_sym2" label="~"/>
    </key>
    <key label="~ ">
      <toggle_state state_id="@string/toggle_en_sym2" label="&lt;"/>
    </key>
     <key label="《">
      <toggle_state state_id="@string/toggle_en_sym2" label="&gt;"/>
    </key>
    <key label="》">
      <toggle_state state_id="@string/toggle_en_sym2" label="€"/>
    </key>
    <key label="$">
      <toggle_state state_id="@string/toggle_en_sym2" label="£"/>
    </key>
    <key label="&amp;">
      <toggle_state state_id="@string/toggle_en_sym2" label="￥"/>
    </key>
	<key label=".">
      <toggle_state state_id="@string/toggle_en_sym2" label="•"/>
    </key>  
  </row>

  <row width="10.205%p">
     <key code="-3" key_type="3" label="123" width="14.286%p"/>
    <key label="..." width="14%p">
      <toggle_state state_id="@string/toggle_en_sym2" label="..."/>
    </key>
    <key label="，" width="14%p">
      <toggle_state state_id="@string/toggle_en_sym2" label="，"/>
    </key>
      <key label="？" width="14%p">
      <toggle_state state_id="@string/toggle_en_sym2" label="？"/>
    </key>
      <key label="！" width="14%p">
      <toggle_state state_id="@string/toggle_en_sym2" label="！"/>
    </key>
      <key label="’" width="14%p">
      <toggle_state state_id="@string/toggle_en_sym2" label="’"/>
    </key>
    <key id="3"/>
  </row>

  <row width="14.696%p" key_type="1">
     <key
            code="-2"
            label="ABC"
            repeat="true"
            width="14.286%p" >
            <toggle_state
                code="-2"
                label="ABC"
                state_id="@string/toggle_en_sym1" />
        </key>
        <key id="8" />
      <!-- <key
            icon="@drawable/voice_normal"
            icon_popup="@drawable/voice_press"
            key_type="2"
            state_id="@string/toggle_en_lower"
            width="10%p" /> -->

        <key
            code="62"
            key_type="5"
            label="space"
            width="40.608%p" />
         <key
            code="-10"
            icon="@drawable/chinese_translated"
            icon_popup="@drawable/english_translated"
            label="译"
            width="10%p" />
        <key id="1" />
  </row>
</keyboard>

